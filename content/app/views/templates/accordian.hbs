<div id="header">
	<div id="header_title" class="title"></div>
</div>

<div id="accordian_page" class="content_wrapper">
	<div id="wrapperAccordian" class="scroll_wrapper">
		<div id="scroller">
			{{#each how-to}}
			<div class="faq_item">
				<div class="question_wrapper">
					<div class="question">{{item-header}}</div>
					<div class="item_arrow"></div>
					<div class="clear"></div>
				</div>
				<div class="answer">{{item-body}}</div>
			</div>
			{{/each}}
		</div>
	</div>
</div>