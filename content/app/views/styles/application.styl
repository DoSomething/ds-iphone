}


/* REGISTER ******************************/
#register_page .scroll_wrapper { 
	bottom:0; 
	background:#efefef;
}

#register_page form {
	width:92%;
	margin:0 auto;
	padding-bottom:10px;
}

#register_page input[type="text"], #register_page input[type="email"], #register_page input[type="number"], #register_page input[type="tel"], #register_page input[type="date"] {
	margin:0;
	width:94%;
	padding:4px 0 4px 10px;
	border-size:1px;
	border-color:#fff;
	border-top-color:#bbb;
	background:#fff;
}

#register_page .button {
	float:left;	
	margin:8px;
	width:128px;
	height:30px;
	line-height:33px;
	font-size:16px;
}

#register_page input[type="submit"] { 
	width:130px;
	height:32px;
}


/* FORMS ******************************/
input[type="text"], input[type="email"], input[type="number"], input[type="tel"], input[type="date"] {
