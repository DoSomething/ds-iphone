@font-face{
  font-family: "DINComp";
  src: url("../DINComp-CondBold.ttf");
}

* { -webkit-tap-highlight-color: rgba(0,0,0,0); }

body,ul,li {
	padding:0;
	margin:0;
	border:0;
}

body {
	font-size:14px;
	-webkit-user-select:none;
  -webkit-text-size-adjust:none;
	font-family:"DINComp";
}

p {
	margin:0;
	padding:7px 0;
}

.h1 { font-size:24px; line-height:28px; }
.h2 { font-size:20px; line-height:24px; }
.h3 { font-size:18px; line-height:20px; }
.h4 { font-size:16px; line-height:20px; }
.h5 { font-size:14px; line-height:18px; }

.centered { margin-left:auto; margin-right:auto; }
.align_center { text-align:center; }
.clear { clear:both; }


/* BUTTONS ******************************/
.button, input[type="submit"] {
	text-align:center;
	border-radius:7px;
	color:#000;
	text-transform:uppercase;
}

.yellow_button { 
	background:-webkit-linear-gradient(top, rgba(255,207,6,1) 0%,rgba(254,205,7,1) 31%,rgba(251,197,11,1) 53%,rgba(245,183,16,1) 74%,rgba(233,159,23,1) 96%,rgba(233,159,23,1) 100%);
	border:1px solid rgb(255,207,6); 
}
.gray_button { 
	background:-webkit-linear-gradient(top, rgba(250,250,250,1) 0%,rgba(188,188,188,1) 100%);
	border:1px solid #ccc;  
}

.disabled_button { color:#999; }

.active_yellow:active, .active_yellow_active { background:-webkit-linear-gradient(top, rgba(233,159,23,1) 0%,rgba(233,159,23,1) 4%,rgba(245,183,16,1) 26%,rgba(251,197,11,1) 47%,rgba(254,205,7,1) 69%,rgba(255,207,6,1) 100%); } /* opposite gradient */
.active_gray:active, .active_gray_active { background:-webkit-linear-gradient(top, rgba(188,188,188,1) 0%,rgba(250,250,250,1) 100%); }

.back_button {
	position:absolute;
	top:8px;
	left:5px;
	width:52px;
	height:31px;
	background-size:cover;
	background-image:url(../images/back.png);
}

.back_button:active {
	background-position:0 -31px;
}


/* HEADER ******************************/
#header {
	position:absolute; z-index:10;
	top:0; left:0;
	width:100%;
	height:45px;
	background-color:#173781;
	padding:0;
	box-shadow:0 -13px 5px 15px #000;
}

#header .logo {
	margin:3px auto 0 auto;
	width:50px;
	height:60px;
}

.logo img {
	width:50px;
	height:60px;
}

.title {
	font-size:24px;
	text-transform:uppercase;
	width:58%;
	margin:0 auto;
	height:45px;
	line-height:50px;
	text-align:center;
	white-space:nowrap;
	overflow:hidden;
	text-overflow:ellipsis;
	color:#fff;
}

/* FOOTER ******************************/
#footer {
	position:absolute; z-index:2;
	bottom:0; left:0;
	width:100%;
	height:45px;
	background-color:#173781;
	padding:0;
	box-shadow:0 15px 5px 15px #000;
}

.tab_wrapper {
	width:106px;
	height:45px;
	overflow:hidden;
}

.tab_wrapper:active, .tab_wrapper_active {
	background:rgba(0,0,0,0.4);
}

.tab_title {
	text-align:center;
	font-size:14px;
	line-height:16px;
	color:#fff;
}

.tab {
	margin:1px auto;
	width:25px;
	height:25px;
	background-size:cover;
	background-position:0 0;
}

#profile_tab {
	position:absolute;
	top:0;
	left:0;
}
#profile_tab .tab {
	background-image:url(../images/profile_tab.png);
}

#getInvolved_tab {
	position:relative;
	margin:0 auto;
}
#getInvolved_tab .tab {
	background-image:url(../images/getInvolved_tab.png);
}

#settings_tab {
	position:absolute;
	top:0;
	right:0;
}
#settings_tab .tab {
	background-image:url(../images/settings_tab.png);
}


/* CONTENT ******************************/
.page {
	margin:45px auto 0 auto;
	width:100%;

}

/*EDU *************/
.edu-image{
	display:block;
	background-size:cover;
	width:320px;
	height:480px;
}

#edu1{ background-image:url(../images/edu1.png); }
#edu2{ background-image:url(../images/edu2.png); }
#edu3{ background-image:url(../images/edu3.png); }
#edu4{ position:relative; background-image:url(../images/edu4.png); }

#edu-wrapper {
	width:100%;
	min-width:320px;
	height:480px;
	background:#ddd;
}

#edu4 .wide_button { 
	position:absolute;
	top:125px;
	left:10%;
	width:80%;
	height:50px;
	font-size:36px;
	line-height:50px;
	text-shadow:1px 2px 3px rgba(0,0,0,0.8);
}

.aqua_button { background-color:#68c4c3; }

#nav {
	position:absolute;
	z-index:100;
	top:8px;
	width:200px;
	height:20px;
	left:50%;
	background:rgba(0,0,0,0.75);
	padding:0;
	margin:0 0 0 -100px;
	-webkit-border-radius:10px;
}

#nav li {
	display:block;
	float:left;
	width:14px;
	height:14px; line-height:14px;
	-webkit-border-radius:7px;
	background:rgba(255,255,255,0.1);
	overflow:hidden;
	padding:0;
	margin:3px 11px 0 0;
	text-align:center;
}

#nav li#prev {
	margin-left:5px;
	background:transparent;
}

#nav li#next {
	margin-right:0;
	background:transparent;
}

#nav li.selected {
	background:rgba(255,255,255,0.4);
}

#swipeview-slider > div {
	position:relative;
	display:-webkit-box;
	-webkit-box-orient:vertical;
	-webkit-box-pack:center;
	-webkit-box-align:center;
	overflow:hidden;
}

#swipeview-slider span {
	-webkit-box-sizing:border-box;
	display:block;
	text-align:center;
	font-size:1.4em;
} 


/* iSCROLL ******************************/
.scroll_wrapper {
	position:absolute; z-index:1;
	top:45px; bottom:45px; left:0;
	width:100%;
	overflow:auto;
	-webkit-transition:0.2s top ease-in-out;
}

#scroller {
	position:absolute; z-index:1;
	width:100%;
	padding:0;
}

.item_arrow {
	position:absolute;
	top:24px;
	right:15px;
	width:16px;
	height:16px;
	background-image:url(../images/list_arrow.png);
	background-size:contain;
}


/* LOGIN ******************************/
#login_page {
	position:absolute;
	top:0; left:0; right:0; bottom:0;
	background:-webkit-linear-gradient(top, rgba(24,64,139,1) 0%,rgba(24,64,139,1) 17%,rgba(34,98,172,1) 17%,rgba(34,98,172,1) 100%);
}

#login_page .bglogo {
	width:100%;
	height:150px;
	background-image:url(../images/login_logo.png);
	background-position:50% 25%;
	background-size:141px 110px;
	background-repeat:no-repeat;
}

#login_page form {
	margin:0 auto 0 auto;
	width:276px;
	padding:15px 0;
	background:#1E1D1C;
	border-top:10px solid #18408B;
}

#login_page input[type="text"], #login_page input[type="email"] {
	margin-left:13px;
	margin-top:5px;
	width:240px;
	padding:7px 0 7px 10px;
	border:0;
	background:#333131;
	color:#999898;
}

#login_page .button {
	margin:5px auto;
	width:220px;
	height:39px;
	line-height:45px;
	font-size:20px;
}

#login_page input[type="submit"] { 
	width:222px;
	height:41px;
	margin-left:27px;
	margin-top:10px;
}

#login_page .facebook_button { 
	margin-top:25px;
	width:180px;
	height:39px;
	background-image:url(../images/facebook_login.png);
